extends ./pug/layout/base

block header
    include ./pug/includes/header
    
block main

    section
        .container
            +thumbnail("oceans-eleven", "Danny Ocean makes an elevator pitch.")
            +thumbnail("pirates", "Introducing Captain Jack Sparrow!")
      
    example-component
        +list
      
         
        +name('name1')
        +name('name2')
        +name('name3')
      

        + news('Новость без текста')
        + news('Новость с текстом')
            p Lorem
            p Родился Иван Сусанин
      
        +link('yandex.ru', 'Яндекс') 
        +link('google.ru', 'Гугл') 
      
        +list('list1','vk','tw','odn')
        +list('list2','vk','tw','odn')
        +list('list3','vk','tw','odn')
